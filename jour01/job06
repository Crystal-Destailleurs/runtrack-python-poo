class Animal:
    def __init__(self):
        self.age = 0
        self.prenom = ""

    def vieillir(self):
        self.age += 1

animal_instance = Animal()


print(f"Âge initial de l'animal : {animal_instance.age}")


animal_instance.vieillir()


print(f"Âge après vieillissement : {animal_instance.age}")
